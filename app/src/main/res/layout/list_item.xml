<?xml version="1.0" encoding="utf-8"?>
<com.pc_logix.huntingloghelper.util.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/my_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <ImageView
        android:id="@+id/logIconView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="48dp"
        android:minHeight="48dp"
        android:background="@mipmap/launcher" />

    <CheckedTextView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/checkedTextView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:gravity="center"
        android:focusable="false"
        android:drawableLeft="?android:attr/listChoiceIndicatorMultiple" />
</com.pc_logix.huntingloghelper.util.CheckableLinearLayout>