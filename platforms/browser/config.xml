<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pc_logix.huntingloghelper" version="1.2.56" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>FFXIV Log Helper</name>
    <description>
        FFXIV Log Helper Hunting, Crafting, Fishing and more!
    </description>
    <author email="support@pc-logix.com" href="http://pc-logix.com">
        PC-Logix
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="BackupWebStorage" value="none" />
</widget>
